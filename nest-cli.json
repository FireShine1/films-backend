{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/profile-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/profile-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/profile-service",
  "projects": {
    "profile-service": {
      "type": "application",
      "root": "apps/profile-service",
      "entryFile": "main",
      "sourceRoot": "apps/profile-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/profile-service/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "films": {
      "type": "application",
      "root": "apps/films",
      "entryFile": "main",
      "sourceRoot": "apps/films/src",
      "compilerOptions": {
        "tsConfigPath": "apps/films/tsconfig.app.json"
      }
    },
    "actors": {
      "type": "application",
      "root": "apps/actors",
      "entryFile": "main",
      "sourceRoot": "apps/actors/src",
      "compilerOptions": {
        "tsConfigPath": "apps/actors/tsconfig.app.json"
      }
    },
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "reviews": {
      "type": "application",
      "root": "apps/reviews",
      "entryFile": "main",
      "sourceRoot": "apps/reviews/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reviews/tsconfig.app.json"
      }
    }
  }
}